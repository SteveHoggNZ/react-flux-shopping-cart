{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node ./tooling/devServer.js",
    "test": "NODE_ENV=production karma start ./tooling/karma.config.js ; exit 0",
    "test:ci": "NODE_ENV=production karma start ./tooling/karma.config.js --single-run",
    "coverage": "npm run coverage:serve && npm run coverage:browse",
    "coverage:serve": "serve --port 3003 --no-logs coverage/lcov/lcov-report &",
    "coverage:browse": "'/Applications/Google Chrome.app/Contents/MacOS/Google Chrome' 'http://localhost:3003' 2>/dev/null",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "fs": "0.0.2",
    "html-webpack-plugin": "^1.6.2",
    "isparta-instrumenter-loader": "^0.2.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-spec-reporter": "0.0.22",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "replacestream": "^4.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "skin-deep": "^0.12.0",
    "tape": "^4.2.2",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "history": "^1.13.1",
    "jquery": "^2.1.4",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0"
  }
}
